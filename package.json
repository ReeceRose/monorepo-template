{
  "name": "monorepo-template",
  "version": "1.1.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:docker": "turbo run build:docker",
    "start:docker": "turbo run start:docker",
    "stop:docker": "turbo run stop:docker",
    "push:docker": "./scripts/push.sh",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:write": "turbo run test -u",
    "prepare": "husky install",
    "pnpm-context": "pnpm/pnpm-context.mjs"
  },
  "devDependencies": {
    "@pnpm/filter-workspace-packages": "^4.4.20",
    "@pnpm/logger": "^4.0.0",
    "globby": "^13.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "meow": "^10.1.2",
    "mississippi": "^4.0.0",
    "prettier": "^2.5.1",
    "tar": "^6.1.11",
    "turbo": "^1.1.2"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.28.0"
}