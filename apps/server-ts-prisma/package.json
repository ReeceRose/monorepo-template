    "@typescript-eslint/parser": "^5.30.5",
    "config": "workspace:*",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lib": "workspace:*",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
