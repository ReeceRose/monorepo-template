{
  "name": "client-graphql",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "dev": "pnpm schema:copy && next dev --port 3001",
    "build": "next build",
    "build:docker": "pnpm --silent --workspace-root pnpm-context -- -p 'tsconfig.json' -p 'bin/' apps/client-graphql/Dockerfile | DOCKER_BUILDKIT=1 docker build --build-arg PACKAGE_PATH=apps/client-graphql - -t monorepo-template-client-graphql",
    "start": "next start",
    "start:docker": "docker run --rm --name=monorepo-template-client-graphql -p 3001:3000 monorepo-template-client-graphql",
    "stop:docker": "docker stop monorepo-template-client-graphql || true",
    "schema:copy": "cp ../server-go-graphql/graph/schema.graphqls graphql/schema.graphql",
    "lint": "next lint",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "test": "jest --passWithNoTests",
    "test:write": "jest -u",
    "test:watch": "jest --watch",
    "schema:generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "next": "13.0.2",
    "next-transpile-modules": "10.0.0",
    "next-urql": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0",
    "react-is": "^18.2.0",
    "ui": "workspace:*",
    "urql": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typed-document-node": "^2.3.6",
    "@graphql-codegen/typescript": "2.8.1",
    "@graphql-codegen/typescript-operations": "2.5.6",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@next/eslint-plugin-next": "^13.0.2",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "config": "workspace:*",
    "eslint": "^8.27.0",
    "eslint-config-next": "^13.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql": "^16.6.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lib": "workspace:*",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.3",
    "tsconfig": "workspace:*",
    "typescript": "^4.8.4",
    "wonka": "^6.1.1"
  }
}