{
  "name": "web",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "build:docker": "pnpm --silent --workspace-root pnpm-context -- -p 'tsconfig.json' -p 'bin/' apps/web/Dockerfile | DOCKER_BUILDKIT=1 docker build --build-arg PACKAGE_PATH=apps/web - -t monorepo-template-web",
    "start": "next start",
    "start:docker": "docker run --rm --name=monorepo-template-web -p 3000:3000 monorepo-template-web",
    "stop:docker": "docker stop monorepo-template-web || true",
    "lint": "next lint",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:write": "jest -u",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "next": "12.1.5",
    "next-transpile-modules": "9.0.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@next/eslint-plugin-next": "^12.1.5",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.30",
    "@types/react": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "autoprefixer": "^10.4.5",
    "babel-jest": "^28.0.3",
    "config": "workspace:*",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "tsconfig": "workspace:*",
    "typescript": "^4.6.4"
  }
}
